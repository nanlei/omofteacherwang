<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="admin-primary" extends="app-default" namespace="/admin/primary">
		<action name="list" class="neo.app.action.admin.PrimaryAction"
			method="list">
			<result name="list">primary_list.ftl</result>
		</action>
		<action name="addConsulting" class="neo.app.action.admin.PrimaryAction"
			method="addConsulting" />
		<action name="info" class="neo.app.action.admin.PrimaryAction"
			method="info">
			<result name="info">primary_info.ftl</result>
		</action>
		<action name="addPost" class="neo.app.action.admin.PrimaryAction"
			method="addPost" />
		<action name="deletePost" class="neo.app.action.admin.PrimaryAction"
			method="deletePost" />
	</package>
</struts>